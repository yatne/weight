{
  "plugins": [
    "react"
  ],
  "rules": {
    "max-len": [
      "error",
      {
        "code": 120
      }
    ],
//    "no-else-return": "off",
    "linebreak-style": "off",
    "class-methods-use-this": "off",
    "no-confusing-arrow": "off",
    "no-use-before-define": "off",
    "object-curly-newline": [
      "error",
      {
        "ObjectExpression": {
          "minProperties": 10,
          "multiline": true,
          "consistent": true
        },
        "ObjectPattern": {
          "minProperties": 10,
          "multiline": true,
          "consistent": true
        }
      }
    ],
    "global-require": "off",
    "import/prefer-default-export": "off",
    "react/forbid-prop-types": "off",
    "react/jsx-filename-extension": "off",
    "react/sort-comp": "off"
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "document": false,
    "require": false,
    "describe": false,
    "test": false,
    "expect": false,
    "beforeEach": false,
    "afterEach": false,
    "window": false,
    "location": false
  }
}
